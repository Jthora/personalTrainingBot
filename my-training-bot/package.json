{
  "name": "my-training-bot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate-module-paths": "npx tsx src/utils/generateModulePaths.tsx",
    "generate-submodule-paths": "npx tsx src/utils/generateSubModulePaths.tsx",
    "generate-carddeck-paths": "npx tsx src/utils/generateCardDeckPaths.tsx",
    "dev": "npm run generate-module-paths && npm run generate-submodule-paths && npm run generate-carddeck-paths && vite",
    "build": "npm run generate-module-paths && npm run generate-submodule-paths && npm run generate-carddeck-paths && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "fs": "^0.0.1-security",
    "howler": "^2.2.4",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5",
    "uuid": "^11.0.5",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.14",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
